{
  "name": "blossom-schoolcatering",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "workspaces": [
    "apps/api",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev:web",
    "dev:web": "vite apps/web --host 0.0.0.0 --port 5173",
    "dev:api": "npm --prefix apps/api run start:dev",
    "build": "npm run build:web && npm run build:api",
    "build:web": "vite build apps/web",
    "build:api": "npm --prefix apps/api run build",
    "preview": "vite preview --host 0.0.0.0 --port 4173",
    "test": "echo \"No tests yet\"",
    "build:types": "npm --prefix packages/types run build",
    "check:config": "npm --prefix packages/config run validate"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-net1io:Gaia-Digital-Agency/blossom_schoolcatering.git"
  },
  "keywords": [],
